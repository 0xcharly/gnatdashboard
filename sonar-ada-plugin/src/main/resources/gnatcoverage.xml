<rules>
    <rule>
        <key>stmt</key>
        <name>statement violation</name>
        <description>Statement has not been executed.</description>
        <tag>gnatcoverage</tag>
    </rule>
    <rule>
        <key>decision</key>
        <name>decision violation</name>
        <description>Some decision outcomes are not exercised.</description>
        <tag>gnatcoverage</tag>
    </rule>
    <rule>
        <key>mcdc</key>
        <name>MC/DC violation</name>
        <description>Some conditions do not have an evaluation pair that proves independent influence.</description>
        <tag>gnatcoverage</tag>
    </rule>
</rules>
