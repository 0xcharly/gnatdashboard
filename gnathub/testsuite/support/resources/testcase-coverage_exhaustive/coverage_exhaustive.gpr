project Coverage_Exhaustive is

   for Object_Dir use "obj";
   for Source_Dirs use ("src");
   for Languages use ("Ada");
   for Main use ("main.adb");

   package Compiler is
      for Default_Switches ("Ada")
        use ("-g", "-fdump-scos", "-fpreserve-control-flow");
   end Compiler;

end Coverage_Exhaustive;
