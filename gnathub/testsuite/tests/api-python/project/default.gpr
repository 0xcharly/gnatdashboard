project default is

   for Object_Dir use "obj";
   for Source_Dirs use (".", "src");

   package Check is
      for Default_Switches ("Ada") use ("-rules", "-from=gnatcheck.rules");
   end Check;

   package Dashboard is
      for Project_Key use "Default::Main";
      for Project_Version use "1.0.0b";
      for Source_Encoding use "My_Encoding";
      for Local_Repository use "repo";
      for Plugins use ("sonarconfig", "codepeer");
      for Plugins_Off use ("gnatprove", "codepeer");
   end Dashboard;

end default;
